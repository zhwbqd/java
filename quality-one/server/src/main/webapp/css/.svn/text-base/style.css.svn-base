@CHARSET "UTF-8";

html {
	min-width: 966px;
	background: #f9f9f9;
	overflow: scroll;
	overflow-x: hidden;
}

body {
	margin: 0;
	padding: 0;
	border: 0;
	text-align: center;
	background: #f9f9f9 url(image/body_bg.gif) top center repeat-y;
	font: 12px Verdana, Arial, Helvetica, sans-serif;
}

nav a:link {
	color: #808080;
	text-decoration: none;
}

nav a:visited {
	color: #808080;
	text-decoration: none;
}

nav a:hover {
	color: #D19275;
	text-decoration: underline;
}

div#box a:link {
	color: #000000;
	text-decoration: none;
}

div#box a:visited {
	color: #000000;
	text-decoration: none;
}

div#box a:hover {
	color: #D19275;
	text-decoration: underline;
}

div#wrapper {
	margin: 0 auto;
	padding: 0;
	border: 0;
	width: 966px;
	text-align: left;
}

div#header {
	margin: 0 0 0 12px;
	padding: 0;
	border: 0;
	width: 937px;
	height: 90px;
	background: #B6B6B6;
}

nav {
	width: 937px;
	height: 30px;
	margin: 0 0 0 12px;
	padding: 5px 0 0 0;
	text-align: center;
}

nav a {
	font-size: 18px;
	font-weight: bold;
	margin-left: 50px;
	margin-right: 50px;
}

div#wrapper div#box {
	width: 937px;
	margin: 0 0 0 12px;
	padding: 0;
	border: 1px dotted black;
	float: none;
}

div#wrapper div#box div.panel {
	width: 450px;
	margin: 5px 5px 5px 5px;
	padding: 0;
	background-color: #DCDCDC;
}

div#wrapper div#box div.panel div.title {
	margin: 0;
	height: 26px;
	padding-top: 6px;
	font-weight: bold;
	font-size: 14px;
	text-align: center;
	color: #FFFFFF;
	background-color: #B6B6B6;
}

div#wrapper div#box div.panel div.content {
	margin: 0;
	padding-bottom: 5px;
}

div#footer {
	margin: 10px 0 10px 12px;
	padding: 0;
	border: 0;
	width: 937px;
	height: 40px;
	background: #B6B6B6;
}